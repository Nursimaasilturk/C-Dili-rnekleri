INPUT

#include<stdio.h>
#define BOYUT 10
int main(){
    int t[BOYUT]={1,3,5,12,6,2,7,9,14,5};
    int i,j;
     
    printf("%s  %6s  %13s\n","ELEMAN","DEÄžER","GRAFÄ°K");
    for(i=0;i<BOYUT;i++){
        printf("%2d%8d              ",i,t[i]);
        for(j=0;j<t[i];j++){
            printf("%c",'*');
        }
        printf("\n");
    }
    return 0;
}

