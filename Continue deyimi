#include<stdio.h>
int main(){
    int num1,num2;
    int i;
    printf("please,enter the numbers:");
    scanf("%d %d",&num1,&num2);
    for(i=num1;i<=num2;i++){
        if(i%5==0){
            if(i%3==0){
                continue;     // Continue deyimi verilen ifadeye uyacak olan terimleri çıktıda vermez atlar yani..
            }
            printf("%d\n",i);
        }
    }
    return 0;
}
